meta {
  name: Create Certificate for RE LB
  type: http
  seq: 2
}

post {
  url: {{basename}}/api/config/namespaces/{{namespace}}/certificates
  body: json
  auth: inherit
}

body:json {
  {
    "metadata": {
      "name": "seg-selfsigned-re-cert",
      "description": "Certificate presented by the Regional Edge's load balancer for HTTPS",
      "disable": false
    },
    "spec": {
      "certificate_url": "string:",
      "private_key": {
        "blindfold_secret_info": {
          "location": "string:"
        }
      },
      "disable_ocsp_stapling": {}
    }
  }
}
